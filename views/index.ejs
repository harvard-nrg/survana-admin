</script>

<!-- study forms -->
<script id="tpl-study-forms" type="text/template">
    <ul class="nav nav-pills nav-stacked well sortable-list">
        <% _.each(items,function(item){ %>
            <%= itemTemplate({
                    'titleTemplate':titleTemplate,
                    'menuTemplate':menuTemplate,
                    'item':item,
                    'group':groups[item.gid],
                    'onlyPublished':onlyPublished
        }) %>
        <% }); %>
    </ul>
</script>

<script id="tpl-study-forms-item" type="text/template">
    <li class="active dropdown">
        <%= titleTemplate(item) %>
        <%= menuTemplate({
                'item':item,
                'group':group,
                'options':true,
                'onlyPublished':onlyPublished
        }) %>
    </li>
</script>

<script id="tpl-study-forms-title" type="text/template">
    <a data-toggle="dropdown" class="dropdown dropdown-toggle">
        <%- title %>
        <span class="label label-warning"><%- version %></span>
        <b class="caret"></b>
    </a>
</script>

<script id="tpl-form-version-menu" type="text/template">
    <ul class="dropdown-menu">
        <!-- break -->
        <!-- <li><span style="border-top:1px solid lightgray;display:block"></span></li> -->
        <% for (var i=0; i<group.length; ++i) { %>
        <% if (onlyPublished && !group[i].published) continue; %>
        <li>
            <a href="#" data-form-id='<%- group[i].id %>' data-form-version="<%- group[i].version %>">
                <%- group[i].title %>
                <span class="label label-warning"><%- group[i].version %></span>
            </a>
        </li>
        <% } %>
        <% if (typeof(options)!=='undefined') { %>
        <li class="divider"></li>
        <li>
            <a href="#" class="form-menu-options"><i class="icon-edit"></i> Options</a>
        </li>
        <% } %>
    </ul>
</script>

<script id="tpl-form-options" type="text/template">
    <form>
        <fieldset>
            <div class="control-group">
                <label class="control-label" for="form-options-store-url">Store URL</label>
                <div class="controls">
                    <div class="input-prepend">
                        <span class="add-on"><i class="icon-globe"></i></span>
                        <input type="text" class="input-xlarge" id="form-options-store-url" name="store-url"
                               placeholder="https://" value="<%- (options)?options['store-url']:'' %>">
                    </div>
                </div>
            </div>
        </fieldset>
        <input type="hidden" value="<%- index %>" name="_index">
    </form>
</script>

<script id="tpl-publishers-menu" type="text/template">
    <ul class="dropdown-menu">
        <% for (var i=0; i<group.length; ++i) { %>
        <li>
            <a href="#">
                <%- group[i].name %>
            </a>
        </li>
        <% } %>
    </ul>
</script>

<script id="tpl-alert" type="text/template">
    <div class="alert">
        <button class="close" data-dismiss="alert">×</button>
        <span class="alert-message"><%- message %></span>
    </div>
</script>

<script id='tpl-modal-alert' type="text/template">
    <div class="modal hide">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">×</button>
        <h3><%- title %></h3>
      </div>
      <div class="modal-body">
        <p><%= message %></p>
      </div>
      <div class="modal-footer">
        <a href="#" class="btn" data-dismiss="modal">Close</a>
      </div>
    </div>
</script>

<script id="tpl-modal-form" type="text/template">
    <div class="modal hide fade" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3><%- title %></h3>
        </div>
        <div class="modal-body">
            <%= form %>
        </div>
        <div class="modal-footer">
            <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
            <button class="btn btn-primary modal-form-save">Save</button>
        </div>
    </div>
</script>

<script id="store-forms" type="text/store">
    {{- JSON.stringify(forms) }}
</script>

<script id="store-studies" type="text/store">
    {{- JSON.stringify(studies) }}
</script>

<script id="store-publishers" type="text/store">
    {{- JSON.stringify(publishers) }}
</script>


<script data-main="{{=config.prefix}}js/app" src="{{=config.prefix}}js/{{=config.lib.require}}/require.js"></script>
<script type="text/javascript">
    //configure RequireJS
    requirejs.config({
        //url to all javascript files
        baseUrl:    "{{= config.prefix }}js",
        //print all the dependencies, in a way that requirejs can reach them
        paths:  {{- JSON.stringify(config.to_requirejs()) }},
        //specify that backbone depends on jquery and returns a global 'Backbone' object
        shim:{
            'backbone':{
                deps:['underscore','jquery'],
                exports:'Backbone'
            }
        }
    });
</script>

  <div style="margin-top:100px;height:50px">
 &nbsp;
  </div>
</body>
</html>
