<script id="tpl-study-list" type="text/template">
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Status</th>
                <th>Created On</th>
            </tr>
        </thead>
        <tbody>
            <% var sortedItems=items.sort( function(a,b) { return b.created_on - a.created_on; } ); %>
            <% _.each(sortedItems,function(item){ %>
                <%= itemTemplate({'study':item}) %>
            <% }); %>
        </tbody>
    </table>
</script>


<script id="tpl-study-list-item" type="text/template">
    <tr>
        <td>
            <a href="#study/<%- study.id %>">
                <%- study.title %>
            </a>
        <td>
            <% if (study.publishers.length) { %>
                <span class="label label-success" rel="tooltip" title="This study has been published" >Published</span>
            <% } else { %>
                <span class="label" rel="tooltip" title="This study has not been published yet">Draft</span>
            <% } %>
        </td>
        <td>
            <%- (new Date(study.created_on)).toLocaleDateString() %>
        </td>
    </tr>
</script>


<script id="tpl-form-list-table" type="text/template">
    <table class="table">
        <thead>
        <tr>
            <th>Name</th>
            <th>Status</th>
        </tr>
        </thead>
        <tbody>
        <% _.each(items,function(group,gid){ %>
        <%= itemTemplate({
                'form': group[group.length-1],
                'publishInfo': publishInfo[gid]
            }) %>
        <% }); %>
        </tbody>
    </table>
</script>

<script id="tpl-form-list-table-item" type="text/template">
    <tr>
        <td>
            <a href="#form/<%- form.id %>">
                <%- form.title %>
            </a>
        </td>
        <td>
        <% if (publishInfo.published) { %>
            <span class="badge badge-success" rel="tooltip" title="Published" >
                <%- publishInfo.published %>
            </span>
        <% } %>

        <% if (publishInfo.drafts) { %>
            <span class="badge" rel="tooltip" title="Drafts">
                <%- publishInfo.drafts %>
            </span>
        <% } %>
        </td>
    </tr>
</script>

