<script id='tpl-study-view' type='text/template'>
    <div class="container">
        <!-- left menu -->
        <div class="row">

            <!-- sidebar -->
            <div id='sidebar' class="span2">
                <div class="sidebar-nav">
                    <ul class="nav nav-list">
                        <li class="nav-header">Menu</li>
                        <li class="active"><a href="#" onclick="return false">Details</a></li>
                        <% if (study.publishers.length) { %>
                            <li><a href="#" onclick="return false">Responses</a></li>
                        <% } %>
                    </ul>
                </div>

                <div class="spacer">&nbsp;</div>


                <div class="sidebar-nav sidebar-form-groups"></div>
            </div>

            <!-- main container -->
            <div class="span6">
                <div class="container">
                    <div class="row span6" id="messages"></div>
                    <div class="row span6">
                        <div class="page-header">
                            <h2><%- study.title %></h2>
                        </div>
                        <div class="well">
                            <div class="row">
                                <div class="span1">ID:</div>
                                <div class="span3"><strong><%- study.id %></strong></div>
                            </div>

                            <% for (var i in study.publishers) {%>
                                <% var server=study.publishers[i], url=study.urls[server]; %>
                                <div class="row">
                                    <div class="span1">
                                        <span class="label" rel="tooltip" title="<%= server %>">
                                        <%= server %>
                                        </span>

                                    </div>
                                    <div class="span3"><a href='<%- url %>'><%- url %></a></div>
                                </div>
                            <% } %>
                        </div>

                        <ul class="nav nav-tabs">
                            <li class="active">
                                <a href="#<%=study.id%>-edit-forms" data-toggle="tab">Forms</a>
                            </li>
                            <li>
                                <a href="#<%=study.id%>-edit-options" data-toggle="tab">Options</a>
                            </li>

                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane active" id="<%=study.id%>-edit-forms">
                                <ol>
                                    <% for(var i = 0; i < study.forms.length; ++i) { %>
                                    <li>
                                        <strong><a href="#form/<%-study.forms[i].id%>"><%- study.forms[i].title %></a></strong>
                                        <code><%- study.forms[i].version %></code>
                                        <% var overrides = study.overrides[i] %>
                                        <% if (overrides && overrides['store-url']) { %>
                                            <a href="#" rel="tooltip" title="<%- overrides['store-url']%>"><i class="icon-globe"></i></a>
                                        <% } %>
                                    </li>
                                    <% } %>
                                </ol>
                            </div>
                            <div class="tab-pane" id="<%=study.id%>-edit-options">
                                Store URL: <%=study['store-url']%>
                            </div>
                        </div>

                        <hr>

                        <% var servers=_.difference(publishers,study.publishers); %>

                        <% if (servers.length) { %>
                            <div class="btn-group inline pull-left">
                                <button class="btn btn-primary btn-medium btn-study-publish"
                                        data-publisher="<%= servers[0] %>" data-loading-text="Publishing...">
                                    <i class="icon-globe icon-white"></i>
                                    Publish
                                </button>
                                <button class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
                                    <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu">
                                    <% for (var i=0;i<servers.length;++i) { %>
                                        <li>
                                            <a class="btn-study-publish" href="#" data-publisher="<%= servers[i] %>">
                                                <% if (i) { %>
                                                    <%= servers[i] %>
                                                <% } else { %>
                                                    <strong><%= servers[i] %></strong> (default)
                                                <% } %>
                                            </a>
                                        </li>
                                    <% } %>
                                    <% if (servers.length>1) { %>
                                        <li class="divider"></li>
                                        <li><a class="btn-study-publish" href="#">All</a></li>
                                    <% } %>
                                </ul>
                            </div>
                        <% } %>

                        <% if (!study.publishers.length) { %>
                        <button class="btn btn-warning btn-medium btn-study-edit" style="margin-left:5px">
                            <i class="icon-edit icon-white"></i> Edit
                        </button>
                        <% } else { %>
                            <button class="btn btn-warning btn-medium btn-study-fork" style="margin-left:5px">
                                <i class="icon-random icon-white"></i> Fork
                            </button>
                        <% } %>

                    </div>
                </div>
            </div>
        </div>
    </div>
</script>

