<script id='tpl-form-create' type='text/template'>
    <div class="container">
        <!-- left menu -->
        <div class="row">

            <!-- sidebar -->
            <div id='sidebar' class="span2">
                <div class="sidebar-nav">
                    <ul class="nav nav-list">
                        <li class="nav-header">Create</li>
                        <li><a href="#study/create">Study</a></li>
                        <li class="active"><a href="#create-form">Questionnaire</a></li>
                    </ul>
                </div>

                <div class="spacer">&nbsp;</div>

                <div class="sidebar-nav sidebar-form-groups"></div>
            </div>

            <!-- main container -->
            <div class="span6">
                <div class="container">
                    <div class="row span6" id="messages"></div>
                    <div class="row span6">
                        <div class="page-header">
                            <h2>Create Questionnaire
                                <small>// expert mode</small>
                            </h2>
                        </div>
                        <form id='create-form' class="form-vertical" onsubmit="return false">
                          <fieldset>
                            <div class="control-group">
                              <label class="control-label" for="form-title">Name</label>
                              <div class="controls">
                                  <input type="text" class="span3" id="form-title" name="title" rel="tooltip" title="Use one or two words">
                              </div>
                              <label class="control-label" for="form-code">Code</label>
                              <div class="controls">
                                  <input type="text" class="span1" id="form-code" name="code" rel="tooltip" title="All response codes will have this prefix">
                                  e.g. <code>DEM</code>, <code>EH</code>, <code>LIFE</code>
                              </div>
                              <label class="control-label" for="form-version">Version</label>
                              <div class="controls">
                                  <input type="text" class="span1" id="form-version" name="version" value="1.0" rel="tooltip" title="The version number is used to keep track of future changes to this questionnaire">
                                  e.g. <code>0.1</code>, <code>first</code>
                              </div>
                            </div>
                          </fieldset>

                          <fieldset>
                          <legend>JSON: <small>// mustache syntax</small></legend>
                            <div id="code-editor-container">&nbsp;</div>
                          </fieldset>
                          <hr>
                          <input type="submit" class="btn btn-primary">
                          <input type="reset" value="Clear" class="btn">
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>

<script id='tpl-form-view' type='text/template'>
    <div class="container">
        <!-- left menu -->
        <div class="row">

            <!-- sidebar -->
            <div id='sidebar' class="span2">
                <div class="sidebar-nav">
                    <ul class="nav nav-list">
                        <li class="nav-header">Menu</li>
                        <li class="active"><a href="#" onclick="return false">Details</a></li>
                    </ul>
                </div>

                <div class="spacer">&nbsp;</div>

                <div id='sidebar-form-versions' class="sidebar-nav"></div>
            </div>

            <!-- main container -->
            <div class="span6">
                <div class="container">
                    <div class="row span6" id="messages"></div>
                    <div class="row span6">
                        <div class="page-header">
                            <h2 style="display:inline"><%- title %>
                                <small>//<%- id %></small>
                            </h2>
                        </div>
                        <!-- Created -->
                        <div class="row">
                            <div class="span1">Created:</div>
                            <div class="span5"><strong><%- (new Date(created_on)).toLocaleDateString() %></strong></div>
                        </div>
                        <!-- Status -->
                        <div class="row">
                            <div class="span1">Status:</div>
                            <div class="span3">
                                <% if (published) { %>
                                <span class="label label-success" rel="tooltip"
                                      title="This form has been published">Published</span>
                                <% } else { %>
                                <span class="label" rel="tooltip"
                                      title="This form has not been published yet">Draft</span>
                                <% } %>
                            </div>
                        </div>
                        <!-- Version -->
                        <div class="row">
                            <div class="span1"><div style="margin-top:10px;">Version:</div></div>
                            <div class="span5">
                                <ul class="nav nav-pills">
                                    <li class="dropdown">
                                        <a class=" dropdown-toggle" data-toggle="dropdown" href="#">
                                            <span class="label label-warning"><%- version %></span>
                                            <b class="caret"></b>
                                        </a>
                                        <ul class='form-versions'></ul>
                                    </li>
                                </ul>
                            </div>
                        </div>

                        <% if (data) { %>
                            <div class="row">
                                <div class="span3">
                                    <button class="btn btn-info btn-medium btn-form-preview" style="margin-left:5px">
                                        <i class="icon-eye-open icon-white"></i> Preview
                                    </button>
                                    <% if (published) { %>
                                    <button class="btn btn-warning btn-medium btn-form-fork">
                                        <i class="icon-random icon-white"></i> Fork
                                    </button>
                                    <% } else { %>
                                    <a class="btn btn-warning btn-medium btn-form-edit" href="#">
                                        <i class="icon-edit icon-white"></i> Edit
                                    </a>
                                    <% } %>
                                </div>
                                <div class="span3" style="text-align:right">
                                    <% if (!published) { %>
                                    <a class="btn btn-primary btn-medium btn-form-publish" href="#">
                                        <i class="icon-globe icon-white"></i> Publish
                                    </a>
                                    <% } %>
                                </div>
                            </div>
                            <hr>
                            <div id="code-viewer-container">&nbsp;</div>
                        <% } else { %>
                            <h4><i class="icon-info-sign"></i> This questionnaire is empty.</h4>
                        <% } %>
                    </div>
                </div>
            </div>
        </div>

        <form action="/form/preview" class="preview" method="post">
            <input type="hidden" value="{}" name='form'>
        </form>
    </div>
</script>


<script id='tpl-form-edit' type='text/template'>
    <div class="container">
        <!-- left menu -->
        <div class="row">

            <!-- sidebar -->
            <div id='sidebar' class="span2">
                <div class="sidebar-nav">
                    <ul class="nav nav-list">
                        <li class="nav-header">Menu</li>
                        <li class="active"><a href="#">Details</a></li>
                    </ul>
                </div>
            </div>

            <!-- main container -->
            <div class="span6">
                <div class="container">
                    <div class="row span6" id="messages"></div>
                    <div class="row span6">
                        <div class="page-header">
                            <h2>Edit <%=title%>
                                <small>// <%- id %></small>
                            </h2>
                        </div>
                        <form id='edit-form' class="form-vertical" onsubmit="return false">
                            <fieldset>
                                <div class="control-group">
                                    <label class="control-label" for="form-title">Name</label>
                                    <div class="controls">
                                        <input type="text" class="span3" id="form-title" name="title" rel="tooltip"
                                               title="Use one or two words" value="<%- title %>">
                                    </div>
                                    <label class="control-label" for="form-code">Code</label>
                                    <div class="controls">
                                        <input type="text" class="span1" id="form-code" name="code" rel="tooltip"
                                               title="All response codes will have this prefix"
                                               value="<%-code%>">
                                        e.g. <code>DEM</code>, <code>EH</code>, <code>LIFE</code>
                                    </div>
                                    <label class="control-label" for="form-version">Version</label>
                                    <div class="controls">
                                        <input type="text" class="span1" id="form-version" name="version"
                                               rel="tooltip" value="<%- version || '1.0' %>"
                                               title="The version number is used to keep track of future changes to this questionnaire">
                                        e.g. <code>0.1</code>, <code>first</code>
                                    </div>
                                </div>
                            </fieldset>

                            <fieldset>
                                <legend>JSON
                                    <a href="https://github.com/vpetrov/survana-admin/wiki/Developer's-Guide-to-Creating-Questionnaires"
                                       target="_blank"><i class="icon-question-sign"></i></a>
                                    <small class="code-editor-message" style="float:right">
                                        <i class="icon-info-sign"></i> Press Ctrl+S or Command+S to save
                                    </small>
                                </legend>
                                <div id="code-editor-container">&nbsp;</div>
                            </fieldset>
                            <hr>
                            <button id='btn-form-edit-save' class="btn btn-success btn-medium" type='submit'>
                                <i class="icon-ok-sign icon-white"></i>
                                Save
                            </button>
                            <a id='btn-form-edit-cancel' class="btn btn-medium" href="#">
                                <i class="icon-remove-circle"></i>
                                Cancel
                            </a>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <form action="/form/preview" class="preview" method="post">
            <input type="hidden" value="{}" name='form'>
        </form>
    </div>

</script>

<script id="tpl-code-editor-message" type="text/template">
    <div><i class="icon-<%- icon %>"></i> <%- message %></div>
</script>

